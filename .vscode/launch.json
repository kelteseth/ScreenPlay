{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "ü™ü Windows Launch",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${command:cmake.launchTargetPath}",
            "args": [],
            "cwd": "${command:cmake.buildDirectory}/bin",
            "preLaunchTask": "CMake: build",
            "internalConsoleOptions": "openOnSessionStart",
            "console": "internalConsole",
            "environment": [
                {
                    "name": "Path",
                    "value": "${env:Path};${workspaceFolder}\\..\\aqt\\6.8.2\\msvc2022_64\\bin\\;${workspaceFolder}\\..\\aqt\\6.8.2\\msvc2022_64\\modules\\;${workspaceFolder}\\..\\aqt\\6.8.2\\msvc2022_64\\qml\\;${workspaceFolder}\\..\\vcpkg\\installed\\x64-windows\\debug\\bin;"
                }
            ],
            "logging": {
                "moduleLoad": true,
                "trace": true
            }
        },
        {
            "name": "üêßlinux Launch",
            "type": "lldb",
            "request": "launch",
            "program": "${command:cmake.launchTargetPath}",
            "args": [],
            "cwd": "${command:cmake.buildDirectory}/bin",
            "env": {
              "LD_LIBRARY_PATH": "${workspaceFolder}/../aqt/6.8.2/gcc_64/lib:${command:cmake.buildDirectory}/ThirdParty/qqcoro/lib:${command:cmake.buildDirectory}/ThirdParty/QArchive:${command:cmake.buildDirectory}/lib:${env:LD_LIBRARY_PATH}",
              "Path": "${env:Path};${workspaceFolder}/../aqt/6.8.2/gcc_64/bin/;${workspaceFolder}/../aqt/6.8.2/gcc_64/lib/;${workspaceFolder}/../aqt/6.8.2/gcc_64/modules/;${workspaceFolder}/../aqt/6.8.2/gcc_64/qml/;${workspaceFolder}/../vcpkg/installed/x64-linux/debug/lib;"
            }
           },
        {
            "name": "üçèmacOS",
            "type": "lldb",
            "request": "launch",
            "program": "${command:cmake.launchTargetPath}",
            "args": [],
            "cwd": "${command:cmake.buildDirectory}/bin",
        },
        {
            "name": "üêç Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true
        }
    ]
}